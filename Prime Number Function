//This code checks if a number is prime by using a function. Returns 1 if the number is prime, if not, it returns 0.
int prime(int n)
{
	int i,b=0;
	for(i=1;i<=n;i++)
	{
		if(n%i==0)
		{
			b+=1;
		}
	}
	if(b==2)
	{
		return 1;
	}
	else
	{
		return 0;	
	}
}
int main(){
	int n;
	scanf("%d",&n);
	printf("%d",prime(n));
	return 0;
}
